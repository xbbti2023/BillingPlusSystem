@{
}
@model Catagory
<h1>Update Expanse</h1>
<form method="post" asp-action="Edit" asp-controller="catagory">

    <input type="hidden" class="form-control" asp-for="Id" />

    <div class="mt-2">
        <label>Catagory</label>
        <input type="text" class="form-control" asp-for="cata" />
        <span asp-validation-for="cata" class="text-danger"></span>
    </div>
    <div class="mt-2">
        <label>Describtion</label>
        <input type="text" class="form-control" asp-for="descr" />
    </div>
    <div class="mt-2">
        <button type="submit" style="float:right" class="btn btn-success">Update</button>
    </div>
</form>
